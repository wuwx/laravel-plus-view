{
    "name": "wuwx/laravel-plus-view",
    "type": "library",
    "keywords": ["laravel", "view"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wu Weixin",
            "email": "wuweixin@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": false
    },
    "require": {
        "php" : "^7.3|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0",
        "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wuwx\\LaravelPlusView\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wuwx\\LaravelPlusView\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wuwx\\LaravelPlusView\\LaravelPlusViewServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
